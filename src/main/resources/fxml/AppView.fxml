<?xml version="1.0" encoding="UTF-8"?>

<?import dad.javafx.componentes.DateChooser?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox fx:id="view" spacing="5.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1">
   <children>
      <DateChooser fx:id="dateChooser" />
      <HBox alignment="CENTER" prefWidth="200.0" spacing="5.0">
         <children>
            <Button fx:id="initBt" mnemonicParsing="false" onAction="#onInitBt" text="Inicializar" />
            <Button fx:id="consultarBt" mnemonicParsing="false" onAction="#onConsultarBt" text="Consultar" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
